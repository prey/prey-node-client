{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    }, 
    "plugins": ["node"],
    "rules": {
        "node/no-deprecated-api": ["error", {
            "version": ">=16.8.0",
            "ignoreModuleItems": [],
            "ignoreGlobalItems": []
        }]
    }
}
