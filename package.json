    "anti",
    "theft",
    "tracking",
    "remote",
    "monitoring",
    "stolen",
    "laptop"
  ],
  "description": "Prey: Laptop Tracking & Data security. Find & Protect lost devices.",
  "engine": "node >= 20.16.0",
  "engines": {
    "node": ">=20.16.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/prey/prey-node-client.git"
  },
  "dependencies": {
    "arch": "^2.1.2",
